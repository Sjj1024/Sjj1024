{
  "manifest_version":3,
  "name":"Focus",
  "description": "这是一个聚焦",
  "version":"0.0.1",
  "icons":{
    "16": "statics/imgs/icon.png",
    "32": "statics/imgs/icon.png",
    "48": "statics/imgs/icon.png",
    "128": "statics/imgs/icon.png"
  },
  "background":{
    "service_worker":"statics/js/background.js"
  },
  "action": {
    "default_icon":{
      "16": "statics/imgs/icon.png",
      "32": "statics/imgs/icon.png",
      "48": "statics/imgs/icon.png",
      "128": "statics/imgs/icon.png"
    }
  },
  "permissions": ["activeTab", "scripting"],
  "commands": {
    "_execute_action": {
      "suggested_key": {
        "default": "Ctrl+U",
        "mac": "Command+U"
      }
    }
  }
}